ALLDIRS = ConEmuPlugin \
          ConEmuC
# ConEmuD
# ConEmu


all: $(ALLDIRS) clean

FICTIVE=_fictive_
$(FICTIVE):

$(ALLDIRS): $(FICTIVE)
	@echo ===========$@
	@$(MAKE) -C $@ -f makefile_gcc

.PHONY: clean
clean:
	@echo final cleaning
	@rm -f ../gcc/common/*.* ../gcc/conemuc/*.* ../gcc/ConEmuPlugin/*.* ../gcc/Plugin/*.* ../gcc/*.*
	@rmdir ../gcc/common ../gcc/conemuc ../gcc/ConEmuPlugin ../gcc/Plugin ../gcc
